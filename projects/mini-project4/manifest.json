{
    "name": "Translator",
    "version": "1.0",
    "description": "It can help you to translate English into Chinese",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
    }],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": "translatorIcon.png"
    }
}